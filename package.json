{
  "name": "omnibug",
  "version": "0.6.0",
  "description": "Omnibug Extension",
  "main": "Gruntfile.js",
  "scripts": {
    "test": "grunt build-test-providers && ava"
  },
  "ava": {
    "src": [
      "test/providers/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": "inherit"
    }
  },
  "babel": {
    "presets": [
      "@ava/stage-4",
      "stage-2"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/MisterPhilip/omnibug.git"
  },
  "keywords": [
    "omnibug"
  ],
  "author": "Philip Lawrence <omnibug@misterphilip.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/MisterPhilip/omnibug/issues"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.0",
    "grunt": "~1.0.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "~1.1.0",
    "grunt-contrib-watch": "^1.0.0",
    "nyc": "^11.4.1"
  },
  "homepage": "https://omnibug.io/",
  "dependencies": {
    "jshint-html-reporter": "^0.2.5"
  }
}
