{
  "name": "omnibug",
  "version": "1.6.0",
  "description": "Omnibug Extension",
  "main": "Gruntfile.js",
  "scripts": {
    "production": "grunt production",
    "beta": "grunt beta",
    "build": "grunt build",
    "test": "grunt build-test-files && nyc --all ava",
    "postinstall": "grunt build"
  },
  "ava": {
    "files": [
      "test/providers/*.js",
      "test/core/*.js"
    ],
    "require": [
      "esm",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          [
            "module:ava/stage-4",
            {
              "modules": false
            }
          ]
        ]
      }
    }
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "istanbul",
          [
            "@babel/plugin-proposal-decorators",
            {
              "legacy": true
            }
          ],
          "@babel/plugin-proposal-function-sent",
          "@babel/plugin-proposal-export-namespace-from",
          "@babel/plugin-proposal-numeric-separator",
          "@babel/plugin-proposal-throw-expressions",
          "@babel/plugin-syntax-dynamic-import",
          "@babel/plugin-syntax-import-meta",
          [
            "@babel/plugin-proposal-class-properties",
            {
              "loose": false
            }
          ],
          "@babel/plugin-proposal-json-strings"
        ]
      }
    }
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "./tests/**",
      "./platform/**",
      "./.github/**",
      "./node_modules/**",
      "**/providers.js"
    ],
    "sourceMap": false,
    "instrument": false,
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/MisterPhilip/omnibug.git"
  },
  "keywords": [
    "omnibug"
  ],
  "author": "Philip Lawrence <philip@omnibug.io>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/MisterPhilip/omnibug/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/register": "^7.4.0",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@ungap/url-search-params": "^0.1.0",
    "ava": "^1.4.1",
    "babel-plugin-istanbul": "^5.1.1",
    "browser-env": "^3.2.6",
    "coveralls": "^3.0.3",
    "grunt": "^1.0.4",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "~1.1.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-text-replace": "^0.4.0",
    "nyc": "^13.3.0",
    "sinon-chrome": "^2.3.2"
  },
  "homepage": "https://omnibug.io/",
  "dependencies": {
    "@babel/preset-stage-2": "^7.0.0",
    "esm": "^3.2.22",
    "grunt-sass": "^2.1.0",
    "jshint-html-reporter": "^0.2.5",
    "node-sass": "^4.11",
    "npm": "^6.9.0",
    "spectre.css": "^0.5.8"
  }
}
